{
  "name": "eslint-config-eshinse",
  "version": "1.1.0",
  "description": "eshinse ESLint config.",
  "homepage": "https://github.com/eshinse/eslint-config-eshinse",
  "author": "Kenneth Sundqvist <kenneth@eshin.se> (https://eshin.se)>",
  "license": "Apache-2.0",
  "main": "index.yaml",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "standard-version",
    "publish:github": "conventional-github-releaser -p angular"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eshinse/eslint-config-eshinse"
  },
  "bugs": {
    "url": "https://github.com/eshinse/eslint-config-eshinse/issues"
  },
  "keywords": [
    "eshinse",
    "eslint",
    "eslintconfig"
  ],
  "peerDependencies": {
    "eslint": "^3.12.0"
  },
  "devDependencies": {
    "conventional-github-releaser": "^1.1.3",
    "cz-conventional-changelog": "^1.2.0",
    "standard-version": "^5.0.2"
  }
}
